.qxss-ul{
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
  margin-bottom: 30rpx;
  text-align: center;
}
.qxss-li{
  width: 144rpx;
  height: 144rpx;
  padding: 2rpx;
  margin: 0 5rpx 20rpx;
  background: url(https://moba.res.netease.com/pc/gw/20180725180621/img/zx/bg_role_avatar_05d3105.png) no-repeat;
}
.qxss-img{
  position: relative;
  display: block;
  width: 144rpx;
  height: 144rpx;
  border-radius: 20rpx;
}
.qxss-img-desx{
  font-size: 28rpx;
  color: #fff0d2;
}
.qxss-img:after{
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url(https://moba.res.netease.com/pc/gw/20180725180621/img/zx/mask_role_avatar_56fb111.png) no-repeat;
  opacity: 1;
}